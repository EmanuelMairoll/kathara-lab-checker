{
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"convergence_time": {
			"type": "number"
		},
		"structure": {
			"type": "string"
		},
		"default_image": {
			"type": "string"
		},
		"test": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"requiring_startup": {
					"type": "array",
					"minItems": 1,
					"items": {
						"type": "string"
					}
				},
				"ip_mapping": {
					"type": "object",
					"additionalProperties": false,
					"patternProperties": {
						"^[a-zA-Z0-9]+$": {
							"type": "object",
							"additionalProperties": false,
							"patternProperties": {
								"^([0-9]+)|lo|(eth\\d+)$": {
									"anyOf": [
										{
											"type": "string",
											"pattern": "^((25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])\\.){3}(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9]?[0-9])/(\\d|[1-2]\\d|3[0-2])$"
										},
										{
											"format": "ipv4"
										},
										{
											"format": "ipv6"
										}
									]
								}
							}
						}
					}
				},
				"daemons": {
					"type": "object"
				},
				"kernel_routes": {
					"type": "object"
				},
				"protocols": {
					"type": "object"
				},
				"reachability": {
					"type": "object",
					"additionalProperties": false,
					"patternProperties": {
						"^[a-zA-Z0-9]+$": {
							"anyOf": [
								{
									"format": "ipv4"
								},
								{
									"format": "ipv6"
								}
							]
						}
					}
				}
			}
		}
	},
	"required": [
		"convergence_time",
		"test"
	]
}